{
  "name": "futurlist-api",
  "description": "The server side of a task/calendar app inspired by bullet journaling.",
  "version": "1.0.0",
  "scripts": {
    "start": "node -r esm server.js",
    "test": "ava",
    "test-watch": "ava --watch",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:undo:all && sequelize db:seed:all",
    "resetdb": "dropdb futurlist && createdb futurlist && yarn migrate"
  },
  "main": "server.js",
  "module": "server.js",
  "author": "Aron Jasso",
  "license": "MIT",
  "ava": {
    "require": [
      "esm"
    ]
  },
  "dependencies": {
    "apollo-server": "^2.25.3",
    "apollo-server-express": "^2.10.1",
    "bcrypt": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-resolvers": "^0.4.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "pg": "^7.18.2",
    "sequelize": "^5.21.5",
    "sequelize-mock": "^0.10.2"
  },
  "devDependencies": {
    "ava": "^3.5.0",
    "easygraphql-tester": "^5.1.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1"
  }
}
